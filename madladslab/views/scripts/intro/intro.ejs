<script>
document.addEventListener('DOMContentLoaded',()=>{
     // Script to move bubbles in the SVG
     const bubble1 = document.getElementById('bubble1');
    const bubble2 = document.getElementById('bubble2');

    // Function to animate the bubbles
    function moveBubble(bubble, initialX, initialY, amplitudeX, amplitudeY, speed) {
        let angle = 0;

        function animate() {
            const x = initialX + Math.sin(angle) * amplitudeX;
            const y = initialY + Math.cos(angle) * amplitudeY;

            bubble.setAttribute('cx', x);
            bubble.setAttribute('cy', y);

            angle += speed;

            requestAnimationFrame(animate);
        }

        animate();
    }

    // Start animation for each bubble
    moveBubble(bubble1, 78.82, 163, 10, 15, 0.03); // Bubble 1 moves in a circular path
    moveBubble(bubble2, 46.32, 168.5, 8, 10, 0.04); // Bubble 2 moves with different amplitude
})





document.addEventListener("DOMContentLoaded", function () {
    const reelsSVG = document.getElementById('reelsSVG');
    const editingSVG = document.getElementById('editingSVG');
    const webAppSVG = document.getElementById('webAppSVG');

    // Add hover animation for #reelsSVG
    reelsSVG.addEventListener('mouseenter', function () {
        reelsSVG.style.transform = 'scale(1.1)';
    });
    reelsSVG.addEventListener('mouseleave', function () {
        reelsSVG.style.transform = 'scale(1)';
    });

    // Click interaction for #editingSVG
    editingSVG.addEventListener('click', function () {
        alert('This SVG: A Custom Scalable Vector Graphic using the power of Adobe Illustrator \n\n Own your graphics! Don\'t license them.');
        editingSVG.style.fill = '#ff0066'; // Change fill color on click
    });

    // Animate #webAppSVG on page load
    webAppSVG.style.transform = 'translateY(-100px)';
    webAppSVG.style.opacity = '0';
    setTimeout(function () {
        webAppSVG.style.transition = 'transform 0.5s ease, opacity 0.5s ease';
        webAppSVG.style.transform = 'translateY(0)';
        webAppSVG.style.opacity = '1';
    }, 500);
});


</script>
